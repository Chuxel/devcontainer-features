#!/bin/bash
set -e
if [ "$(id -u)" -ne 0 ]; then
    echo -e 'Script must be run as root.'
    exit 1
fi

# Import common utils
. "$(cd "$(dirname "${BASH_SOURCE[0]}")/../../.." && pwd)/common/utils.sh"

check_command curl curl ca-certificates
check_command tar tar
check_command git git
check_command sha256sum coreutils
