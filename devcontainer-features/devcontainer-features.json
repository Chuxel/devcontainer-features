{
	"features": [
		{
			"id": "vscode",
			"name": "Visual Studio Code",
			"options": {
				"version": {
					"type": "string",
					"enum": [
						"latest"
					],
					"default": "latest",
					"description": "VS Code version (currently ignored)"
				},
				"edition": {
					"type": "string",
					"enum": [
						"stable",
						"insiders",
						"both"
					],
					"default": "stable",
					"description": "VS Code edition to install"
				}
			}
		},
		{
			"id": "googlechrome",
			"name": "Google Chrome",
			"options": {
				"version": {
					"type": "string",
					"enum": [
						"latest"
					],
					"default": "latest",
					"description": "Chrome version (currently ignored)"
				}
			}
		},
		{
			"id": "packcli",
			"name": "Buildpacks.io pack CLI",
			"options": {
				"version": {
					"type": "string",
					"proposals": [
						"latest"
					],
					"default": "latest",
					"description": "Pack CLI version"
				},
				"targetPath": {
					"type": "string",
					"proposals": [
						"/usr/local"
					],
					"default": "/usr/local",
					"description": "Target install path"
				}
			}
		},
		{
			"id": "python",
			"name": "Python",
			"options": {
				"version": {
					"type": "string",
					"proposals": [
						"latest"
					],
					"default": "latest",
					"description": "Python version"
				},
				"targetPath": {
					"type": "string",
					"proposals": [
						"/usr/local"
					],
					"default": "/usr/local",
					"description": "Target install path"
				},
				"buildMode": {
					"type": "string",
					"enum": [
						"production",
						"devcontainer"
					],
					"default": "devcontainer",
					"description": "Select the production build mode to skip installing development tools."
				}
			},
			"extensions": [
				"ms-python.python",
				"ms-python.vscode-pylance"
			]
		},
		{
			"id": "nodejs",
			"name": "Node.js",
			"options": {
				"version": {
					"type": "string",
					"proposals": [
						"lts",
						"latest",
						"16",
						"14",
						"12"
					],
					"default": "lts",
					"description": "Select or enter a Node.js version to install"
				},
				"installYarn": {
					"type": "boolean",
					"default": true,
					"description": "Install yarn"
				},
				"installNvm": {
					"type": "boolean",
					"default": true,
					"description": "Install nvm"
				},
				"nodeGypDependencies": {
					"type": "boolean",
					"default": true,
					"description": "Install dependencies to compile native node modules (node-gyp)?"
				},
				"targetPath": {
					"type": "string",
					"proposals": [
						"/usr/local"
					],
					"default": "/usr/local",
					"description": "Target install path"
				},
				"buildMode": {
					"type": "string",
					"enum": [
						"production",
						"devcontainer"
					],
					"default": "devcontainer",
					"description": "Select the production build mode to skip installing development tools."
				}
			},
			"extensions": [
				"dbaeumer.vscode-eslint"
			]
		},
		{
			"id": "buildpack-test",
			"name": "Test feature for devpacker",
			"containerEnv": {
				"TEST": "true",
				"PATH": "/i/am/before:${PATH}:/i/am/after",
				"TEST2": "${TEST}"
			},
			"init": true,
			"capAdd": [
				"SYS_PTRACE"
			],
			"securityOpt": [
				"seccomp=unconfined"
			],
			"extensions": [
				"mutantdino.resourcemonitor"
			],
			"entrypoint": "/usr/local/etc/dev-container-features/entrypoint-bootstrap.sh",
			"options": {
				"version": {
					"type": "string",
					"proposals": [
						"latest"
					],
					"default": "latest",
					"description": "Version"
				},
				"foo": {
					"type": "string",
					"proposals": [
						"foo"
					],
					"default": "foo",
					"description": "Foo"
				},
				"targetPath": {
					"type": "string",
					"proposals": [
						"/usr/local"
					],
					"default": "/usr/local",
					"description": "Target install path"
				},
				"buildMode": {
					"type": "string",
					"enum": [
						"production",
						"devcontainer"
					],
					"default": "devcontainer",
					"description": "Select the production build mode to skip installing development tools."
				}
			}
		},
		{
			"id": "buildpack-test-2",
			"name": "Test feature #2 for devpacker",
			"containerEnv": {
				"TEST2": "true",
				"PATH": "/i/am/before-2:${PATH}:/i/am/after-2",
				"TEST2-2": "${TEST2}"
			},
			"privileged": true,
			"capAdd": [
				"SYS_PTRACE"
			],
			"securityOpt": [
				"seccomp=unconfined"
			],
			"extensions": [
				"mutantdino.resourcemonitor",
				"chrisdias.vscode-opennewinstance"
			],
			"entrypoint": "/usr/local/etc/dev-container-features/entrypoint-bootstrap.sh",
			"options": {
				"version": {
					"type": "string",
					"proposals": [
						"latest"
					],
					"default": "latest",
					"description": "Version"
				},
				"foo": {
					"type": "string",
					"proposals": [
						"foo"
					],
					"default": "foo",
					"description": "Foo"
				},
				"targetPath": {
					"type": "string",
					"proposals": [
						"/usr/local"
					],
					"default": "/usr/local",
					"description": "Target install path"
				},
				"buildMode": {
					"type": "string",
					"enum": [
						"production",
						"devcontainer"
					],
					"default": "devcontainer",
					"description": "Select the production build mode to skip installing development tools."
				}
			}
		},
		{
			"id": "setup-python",
			"name": "Setup Python",
			"options": {
				"version": {
					"type": "string",
					"proposals": [
						"latest"
					],
					"default": "latest",
					"description": "Python version"
				},
				"installTools": {
					"type": "boolean",
					"default": true,
					"description": "Install developer tools?"
				},
				"targetPath": {
					"type": "string",
					"proposals": [
						"/usr/local"
					],
					"default": "/usr/local",
					"description": "Target install path"
				},
				"buildMode": {
					"type": "string",
					"enum": [
						"production",
						"devcontainer"
					],
					"default": "devcontainer",
					"description": "Select the production build mode to skip installing development tools."
				}
			},
			"extensions": [
				"ms-python.python",
				"ms-python.vscode-pylance"
			]
		},
		{
			"id": "setup-go",
			"name": "Setup Go",
			"options": {
				"version": {
					"type": "string",
					"proposals": [
						"latest"
					],
					"default": "latest",
					"description": "Go version"
				},
				"installTools": {
					"type": "boolean",
					"default": true,
					"description": "Install developer tools?"
				},
				"targetPath": {
					"type": "string",
					"proposals": [
						"/usr/local"
					],
					"default": "/usr/local",
					"description": "Target install path"
				},
				"buildMode": {
					"type": "string",
					"enum": [
						"production",
						"devcontainer"
					],
					"default": "devcontainer",
					"description": "Select the production build mode to skip installing development tools."
				}
			},
			"capAdd": [
				"SYS_PTRACE"
			],
			"securityOpt": [
				"seccomp=unconfined"
			],
			"settings": {
				"go.toolsManagement.checkForUpdates": "local",
				"go.useLanguageServer": true
			},
			"extensions": [
				"golang.Go"
			]
		},
		{
			"id": "setup-java",
			"name": "Setup Java",
			"options": {
				"version": {
					"type": "string",
					"proposals": [
						"latest"
					],
					"default": "latest",
					"description": "Java version"
				},
				"distribution": {
					"type": "string",
					"proposals": [
						"microsoft",
						"temurin",
						"zulu",
						"liberica"
					],
					"default": "microsoft",
					"description": "Java distribution version"
				},
				"javaPackage": {
					"type": "string",
					"proposals": [
						"jdk",
						"jre",
						"jdk+fx",
						"jre+fx"
					],
					"default": "jdk",
					"description": "The package type (jdk, jre, jdk+fx, jre+fx)"
				},
				"targetPath": {
					"type": "string",
					"proposals": [
						"/usr/local"
					],
					"default": "/usr/local",
					"description": "Target install path"
				},
				"buildMode": {
					"type": "string",
					"enum": [
						"production",
						"devcontainer"
					],
					"default": "devcontainer",
					"description": "Select the production build mode to skip installing development tools."
				}
			},
			"extensions": [
				"vscjava.vscode-java-pack"
			]
		},
		{
			"id": "setup-pack",
			"name": "Setup pack CLI",
			"options": {
				"version": {
					"type": "string",
					"proposals": [
						"0.24.0"
					],
					"default": "0.24.0",
					"description": "Pack CLI version"
				},
				"targetPath": {
					"type": "string",
					"proposals": [
						"/usr/local"
					],
					"default": "/usr/local",
					"description": "Target install path"
				}
			}
		}
	]
}